{
  "id": "moduleTask",
  "name": "moduleTask",
  "title": "Fluid-task Editor",
  "size": 100,
  "glyph": "fa fa-gears",
  "active": true,
  "locked": false,
  "closeable": false,
  "showToolBar": true,
  "lazyLoad": true,
  "moduleJS": "moduleTask",
  "moduleFiles": "docs/module_task/module-task.js,docs/module_task/module-task.css,docs/module_task/mode-html.js,docs/module_task/worker-html.js,docs/module_task/theme-eclipse.js",
  "toolbarItems": [
    {
      "id": "createTaskToolbar",
      "label": "New task",
      "uiClass": "btn btn-info",
      "glyph": "fa fa-plus"
    },
    {
      "id": "saveTaskToolbar",
      "label": "Save",
      "uiClass": "btn btn-success",
      "glyph": "fa fa-save"
    },
    {
      "id": "addNewPage",
      "label": "New page",
      "uiClass": "btn btn-default",
      "glyph": "fa fa-edit"
    },
    {
      "id": "removeAllPages",
      "label": "Remove all pages",
      "uiClass": "btn btn-danger",
      "glyph": "fa fa-trash"
    },
    {
      "id": "removePage",
      "label": "Remove page",
      "uiClass": "btn btn-danger",
      "glyph": "fa fa-remove"
    }
  ],
  "pages": [
    {
      "id": 1,
      "name": "taskOverview",
      "title": "Task Overview",
      "static": false,
      "html": "<div ng-controller='moduleTaskCtrl'><h1>I am static: {{hello}}</h1></div>",
      "home": "docs/module_task/pages/task-overview.html",
      "isHome": true,
      "ajax": {
        "isArray": true,
        "url": "http://localhost:8080/fluid-services/services/task_service",
        "auto": true
      },
      "watch": "taskEditor"
    },
    {
      "id": 2,
      "name": "taskEditor",
      "title": "Task Editor",
      "home": "docs/module_task/pages/task-editor.html",
      "ajax": {
        "url": "http://localhost:8080/fluid-services/services/task_service/",
        "auto": true
      }
    }
  ]
}