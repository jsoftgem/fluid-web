{
  "id": "moduleBasic",
  "name": "module_basic",
  "title": "Basic - Locked",
  "size": 25,
  "glyph": "fa fa-gears",
  "useImg": true,
  "imgSrc": "docs/module_basic/icon.png",
  "active": true,
  "locked": false,
  "closeable": false,
  "showToolBar": false,
  "lazyLoad": true,
  "moduleJS": "moduleBasic",
  "moduleFiles": "docs/module_basic/module-basic.js",
  "pageTransition": "bounceIn",
  "pages": [
    {
      "id": 1,
      "name": "page1",
      "title": "Home",
      "static": true,
      "html": "<div ng-controller='moduleBasicCtrl'><ul class='list-group'><li ng-repeat='pg in data'><p>id: {{pg.id}} <br/> name: {{pg.name}} <br/> title: {{pg.title}} <br/></p> <button class='btn btn-info' type='button' ng-click='pg.$save()'><i class='fa fa-save'></i></button></li></ul></div>",
      "isHome": true,
      "ajax": {
        "isArray": true,
        "auto": true,
        "url": "docs/module_basic/pages/page1-data.json"
      }
    }
  ]
}